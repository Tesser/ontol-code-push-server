{
  "name": "code-push-server",
  "version": "0.0.1",
  "description": "Source code for the CodePush service API",
  "main": "./script/server.js",
  "scripts": {
    "start": "node ./bin/server.js",
    "start:env": "npm run build && pm2 start ecosystem.config.js",
    "start:dev": "node -r dotenv/config ./bin/server.js dotenv_config_path=.env dotenv_config_silent=true",
    "clean": "rimraf bin",
    "build": "npm run clean && tsc && shx cp -r ./script/views ./bin/script",
    "prettier": "prettier --write \"./**/*.ts\"",
    "lint": "npx eslint ./script/**/*.ts",
    "lint:fix": "npx eslint ./script/**/*.ts --fix"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/microsoft/code-push-server"
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=9.0.0"
  },
  "author": "Microsoft",
  "dependencies": {
    "@aws-sdk/client-cloudfront": "^3.764.0",
    "@aws-sdk/client-s3": "^3.758.0",
    "@aws-sdk/s3-request-presigner": "^3.758.0",
    "body-parser": "^1.20.2",
    "dotenv": "^16.4.7",
    "ejs": "^3.1.10",
    "email-validator": "1.0.3",
    "express": "^4.21.2",
    "express-domain-middleware": "0.1.0",
    "express-rate-limit": "^7.4.0",
    "ioredis": "^5.6.0",
    "jsonwebtoken": "^9.0.2",
    "mongodb": "^6.14.2",
    "multer": "^1.4.5-lts.1",
    "node-deepcopy": "0.1.1",
    "q": "^1.4.1",
    "redis": "2.4.2",
    "sanitize-html": "^2.11.0",
    "semver": "^7.5.3",
    "shortid": "2.2.2",
    "stream-to-array": "^2.3.0",
    "streamifier": "0.1.1",
    "superagent": "^8.0.9",
    "try-json": "1.0.0",
    "yauzl": "2.6.0",
    "yazl": "2.2.2"
  },
  "devDependencies": {
    "@types/express": "^4.17.17",
    "@types/jsonwebtoken": "^9.0.9",
    "@types/multer": "^1.4.12",
    "@types/node": "^20.1.4",
    "@types/q": "^1.5.5",
    "@types/redis": "^4.0.11",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "eslint": "^8.45.0",
    "prettier": "^2.8.8",
    "rimraf": "^5.0.1",
    "shx": "^0.3.4",
    "typescript": "^5.0.4"
  },
  "resolutions": {
    "@types/express": "4.17.17",
    "@types/express-serve-static-core": "4.17.35",
    "@types/multer": "1.4.12"
  }
}
